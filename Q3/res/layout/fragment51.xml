android:layout_height="wrap_content"        android:orientation="vertical"        >        <TextView             android:layout_width="wrap_content"            android:layout_height="wrap_content"               style="@style/text2d"               android:id="@+id/mTv1d1"               android:text="@string/c75"            />        <TextView             android:layout_width="wrap_content"            android:layout_height="wrap_content"               style="@style/text24"               android:layout_marginTop="10dp"               android:id="@+id/mTv1d2"               android:text="@string/c76"            />        </LinearLayout>      </RelativeLayout>            </RelativeLayout>                                        </LinearLayout>    </ScrollView>