<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:background="@color/www2"    android:fillViewport="true"        android:layout_height="match_parent"    android:orientation="vertical"   >    <LinearLayout         android:layout_width="fill_parent"        android:layout_height="fill_parent"        android:layout_marginLeft="15dp"        android:layout_marginRight="15dp"        android:layout_marginTop="20dp"        android:orientation="horizontal"        >            <RelativeLayout         android:layout_weight="1"        android:layout_height="fill_parent"        android:layout_width="0dp"        >          <RelativeLayout              android:layout_width="fill_parent"              android:layout_height="40dp"              android:id="@+id/mRl1"              android:layout_marginTop="20dp"              >              <ImageView                   android:layout_centerHorizontal="true"                  android:layout_width="wrap_content"                  android:layout_height="40dp"                  android:src="@drawable/l1"                  />          </RelativeLayout>                      <View                   android:layout_width="1dp"                                    android:layout_centerHorizontal="true"                  android:layout_below="@id/mRl1"                   android:id="@+id/mIv2"                  android:layout_height="60dp"                  android:background="@color/www1"                  />                    <RelativeLayout              android:layout_width="fill_parent"              android:layout_height="40dp"              android:id="@+id/mRl2"              android:layout_below="@id/mIv2"              >              <ImageView                   android:layout_centerHorizontal="true"                  android:layout_width="wrap_content"                  android:layout_height="40dp"                  android:src="@drawable/l2"                  />          </RelativeLayout>                      <View                   android:layout_width="1dp"                                    android:layout_centerHorizontal="true"                  android:layout_below="@id/mRl2"                   android:id="@+id/mIv3"                  android:layout_height="60dp"                  android:background="@color/www1"                  />                                                                           <RelativeLayout              android:layout_width="fill_parent"              android:layout_height="40dp"              android:id="@+id/mRl3"              android:layout_below="@id/mIv3"              >              <ImageView                   android:layout_centerHorizontal="true"                  android:layout_width="wrap_content"                  android:layout_height="40dp"                  android:src="@drawable/l3"                  />          </RelativeLayout>                      <View                   android:layout_width="1dp"                                    android:layout_centerHorizontal="true"                  android:layout_below="@id/mRl3"                   android:id="@+id/mIv4"                  android:layout_height="60dp"                  android:background="@color/www1"                  />                   <RelativeLayout              android:layout_width="fill_parent"              android:layout_height="40dp"              android:id="@+id/mRl4"              android:layout_below="@id/mIv4"              >              <ImageView                   android:layout_centerHorizontal="true"                  android:layout_width="wrap_content"                  android:layout_height="40dp"                  android:src="@drawable/l4"                  />          </RelativeLayout>                      <View                   android:layout_width="1dp"                             